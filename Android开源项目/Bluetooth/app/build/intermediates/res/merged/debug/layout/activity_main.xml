<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/load_background"
    tools:context="com.admin.bluetooth.MainActivity">

    <TextView
        android:text="Balance P"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/BP" />

    <Button
        android:text="降低"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/BP"
        android:layout_alignParentStart="true"
        android:layout_marginTop="14dp"
        android:id="@+id/BPD" />

    <Button
        android:text="增加"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/BPD"
        android:layout_alignParentEnd="true"
        android:id="@+id/BPF" />

    <TextView
        android:text="Balance D"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/BPD"
        android:layout_alignParentStart="true"
        android:layout_marginTop="10dp"
        android:id="@+id/BD" />

    <Button
        android:text="降低"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/BD"
        android:layout_alignParentStart="true"
        android:layout_marginTop="16dp"
        android:id="@+id/BDD" />

    <Button
        android:text="增加"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/BDD"
        android:layout_alignStart="@+id/BPF"
        android:id="@+id/BDF" />

    <TextView
        android:text="Velocity P"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/BDD"
        android:layout_alignParentStart="true"
        android:layout_marginTop="17dp"
        android:id="@+id/VP" />

    <Button
        android:text="降低"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/VP"
        android:layout_alignEnd="@+id/BDD"
        android:layout_marginTop="22dp"
        android:id="@+id/VPD" />

    <Button
        android:text="增加"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/VPD"
        android:layout_alignStart="@+id/BDF"
        android:id="@+id/VPF" />

    <TextView
        android:text="Velocity I"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/VPD"
        android:layout_alignParentStart="true"
        android:layout_marginTop="32dp"
        android:id="@+id/textView13" />

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/BPD"
        android:id="@+id/BPN" />

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/BD"
        android:layout_toEndOf="@+id/BPD"
        android:id="@+id/BDN" />

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/VID"
        android:layout_alignEnd="@+id/VPN"
        android:id="@+id/VIN" />

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/VPN"
        android:layout_above="@+id/VPD"
        android:layout_toEndOf="@+id/BDD" />

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ZPD"
        android:layout_toEndOf="@+id/ZPD"
        android:id="@+id/ZPN" />

    <TextView
        android:text="ZP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/ZPD"
        android:layout_alignParentStart="true"
        android:id="@+id/zp" />

    <Button
        android:text="降低"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/VID"
        android:layout_below="@+id/textView13"
        android:layout_alignParentStart="true" />

    <Button
        android:text="增加"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/VIF"
        android:layout_below="@+id/VIN"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="增加"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ZPF"
        android:layout_below="@+id/VIF"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="15dp" />

    <Button
        android:text="降低"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ZPD"
        android:layout_alignBaseline="@+id/ZPF"
        android:layout_alignBottom="@+id/ZPF"
        android:layout_alignParentStart="true" />

    <Button
        android:text="刷新"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ZPF"
        android:layout_alignStart="@+id/ZPF"
        android:layout_marginTop="10dp"
        android:id="@+id/Refresh" />

    <TextView
        android:text="速度"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Velocity"
        android:layout_alignBaseline="@+id/Refresh"
        android:layout_alignBottom="@+id/Refresh"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/V"
        android:layout_alignBaseline="@+id/Velocity"
        android:layout_alignBottom="@+id/Velocity"
        android:layout_toEndOf="@+id/BD" />

</RelativeLayout>
